            GROUP BY AP.PRD_SEQ, TRNS.FROM_PORT
            ) TC on TC.prd_seq=prd.prd_seq and TC.port_seq=prt.port_seq
            left outer join(
            SELECT AP.PRD_SEQ, TRNS.FROM_PORT PORT_SEQ,
